[user]
	name = ycanerol
	email = ycanerol@gmail.com
[core]
	excludesfile = /Users/ycan/.gitignore_global
[commit]
	verbose = True
[alias]
	st = status
	df = diff
	br = branch
	co = checkout
	cob = checkout -b
	logp = log --graph --oneline --decorate --all
	logpp = log --graph --abbrev-commit --decorate --format=format:'%C(blue)%h%C(reset) - %C(bold cyan)%ad%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset) %C(dim white)- %an%C(reset) %n''          %C(white)%s%C(reset) ' --all --date=format:'%Y-%m-%d %H:%M'
	glop = logp
[merge]
	tool = nvim -d
[pull]
	ff = only
